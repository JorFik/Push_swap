	"version": "0.2.0",
	"configurations": [
		{
			"name": "Dbug",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/debugger/a.out",
			"args": ["2", "1", "3", "6", "5", "8"],
			"cwd": "${workspaceFolder}",
			"breakpointMode": "file",
		},
		{
			"name": "Debug with changing args",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/debugger/a.out",
			"args": ["8 5 6 3 1 2"],
			"cwd": "${workspaceFolder}",
			"breakpointMode": "file",
		}
	]
}